{
    "name": "pownall/read-time",
    "description": "This is my package read-time",
    "keywords": [
        "leepownall",
        "read-time"
    ],
    "homepage": "https://github.com/leepownall/read-time",
    "license": "MIT",
    "authors": [
        {
            "name": "Lee Pownall",
            "email": "lee@pownall.uk",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/contracts": "^10.0",
        "nesbot/carbon": "^2.0",
        "spatie/laravel-package-tools": "^1.16"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "orchestra/testbench": "*",
        "pestphp/pest": "^2.15",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "Pownall\\ReadTime\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pownall\\ReadTime\\Tests\\": "tests",
            "Workbench\\App\\": "workbench/app/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@composer run prepare",
        "clear": "@php vendor/bin/testbench package:purge-skeleton --ansi",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "build": [
            "@composer run prepare",
            "@php vendor/bin/testbench workbench:build --ansi"
        ],
        "start": [
            "Composer\\Config::disableProcessTimeout",
            "@composer run build",
            "@php vendor/bin/testbench serve"
        ],
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Pownall\\ReadTime\\ReadTimeServiceProvider"
            ],
            "aliases": {
                "ReadTime": "Pownall\\ReadTime\\Facades\\ReadTime"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
